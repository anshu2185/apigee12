node {
   echo 'SharedFlow Deployment started...'
  
	stage ('Checkout'){
		checkout scm
	}
	
	stage ('Deploying SharedFlow'){
		def mvnHome = tool 'Maven3.3.9'
	    sh "${mvnHome}/bin/mvn --version"
		echo "${WORKSPACE}"
		def  dirsl = [] 
    new File("${WORKSPACE}").eachDir()
    {
        dirs -> println dirs.getName()
    }
	}

}
